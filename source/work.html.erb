<section class="<%=name.parameterize%> work__detail">

  <div class="work__content">
    <div class="work__summary">

      <div class="work__header">
        <%= link_to("BACK TO HOME", "/" ) %>
      </div>

      <%= image_tag("logos/#{name.parameterize}.png", :class => "work__logo") %>

      <a href="http://<%=url%>" target="_blank" class="work__site"><%=url%></a>

      <p class="work__desc"><%=desc%></p>

      <div class="work__nav">
        <div class="work__pre">
          PREVIOUS

          
          
        </div>

        <div class="work__next">
          NEXT
        </div>
      </div>
    </div>

    <div class="work__photos">

      <% photos.each do |f| %>
        <%= image_tag("works/#{name.parameterize}/#{f.photo}.png") %> 
      <% end %>
    </div>
  </div>

  <!-- <%=name%> -->
</section>
